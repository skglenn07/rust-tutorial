{
  "name": "Rust Dev Container",
  "image": "mcr.microsoft.com/vscode/devcontainers/rust:latest",  
  "customizations": {
    "vscode": {
      "settings": {
        "rust-analyzer.cargo.runBuildScripts": true,
        "rust-analyzer.procMacro.enable": true
      },
      "extensions": [
        "rust-lang.rust-analyzer"
      ]
    }
  }
}
